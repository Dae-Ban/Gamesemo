<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.ScrapMapper">
	<delete id="steamDCClean">
		DELETE FROM steam_dc
	</delete>
	<insert id="steamDCInsert" parameterType="scrap">
		INSERT INTO steam_dc 
		VALUES(#{pk},#{title},#{thumb},#{price},#{fprice},#{rate},#{link},#{scrapedAt})
	</insert>
</mapper>