<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.YouTubeVideoMapper">





	<select id="getOrFetchReviewVideos" parameterType="int"
		resultType="com.example.demo.model.YouTubeVideo">
		SELECT * FROM REVIEW_VIDEO WHERE G_NUM = #{gnum}
	</select>

	<insert id="insertVideo" parameterType="com.example.demo.model.YouTubeVideo">
		INSERT INTO REVIEW_VIDEO (
		NUM, G_NUM, VIDEO_ID, TITLE, THUMBNAIL_URL
		) VALUES (
		REVIEW_VIDEO_SEQ.NEXTVAL,
		#{gnum},
		#{video.videoId},
		#{video.title},
		#{video.thumbnailUrl}
		)
	</insert>



</mapper>